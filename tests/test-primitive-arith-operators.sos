(* arithmetic operators on primitives int, float *)

add(x: int, y: int) -> int = x + y
int sub(x: int, y: int) -> int = x - y
int mult(x: int, y: int) -> int = x * y
int div(x: int, y: int) -> int = x/y
int mod(x: int, y: int) -> int = x%y

add(x: float, y: float) -> float = x + y 
sub(x: float, y: float) -> float = x - y 
mult(x: float, y: float) -> float = x * y 
div(x: float, y: float) -> float = x/y 

x: int = add(1,2)
print(x) //3
x = sub(1,2)
print(x) //-1
x = mult(1,2)
print(x) //2
x = div(1,2)
print(x) //0
x = mod(10,8)
print(x) //2

y: float = add(1.1, 2.1)
printf(y) //3.2
y = sub(2.1, 1.1)
printf(y) //1.0
y = mult(1.1, 2.1)
printf(y) //2.31
y = div(2.1, 1.1)
printf(y) //1.9090...
