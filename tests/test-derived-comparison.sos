/* structs, not arrays, allows for comparions */

//comparison of unnamed structs
x: bool = {1,2} == {1,2}
print(x) //true
x = {1, 3} == {2, 4}
print(x)

//comparison of named structs
struct point = {x: float, y: float}
p1: point = {1.0, 2.0}
p2: point = {1.0, 2.0}
p3: point = {0.0, 0.0}
x = p1 == p2
print(x) //true
x = p1 == p2
print(x) //true
x = p1 == p3
print(x) //false


