/* 
right associative: =, !, **
left associative: ., *, /, %, +, -, @, of, (comparison), (boolean), ",", ;
*/

/*right*/
//=
a: int = 1
print(a) //1
b: int = a = 2 //b = (a=2) = 2
print(a) //2
print(b) //2

//!
b: bool = true
print(b) //1
print(!b) //0
print(!!b) //1

//left
print(5 - 3 - 1) //2-1 = 1
print((5 - 3) - 1) //2-1 = 1
print(5 - (3 - 1)) //5-2=3

print(100/10/5) // 10/5 = 2
print((100/10)/5) //10/5 = 2
print(100/(10/5)) //100/2 = 50
