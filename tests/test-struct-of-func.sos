//defining function in struct
add: (x: int, y: int) -> int = x + y
addasvar: func int, int -> int = add

binaryfunc: (x: int, y: int, f: func int, int -> int) -> int = f(x, y)

struct s = {
    field: int, 
    field2: func int, int -> int
    }

s1: s = {5, addasvar}
print(binaryfunc(1, 2, s1.field2)) //3

