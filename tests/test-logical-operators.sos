/* logical operators: AND, OR, NOT  */

bool t = True
bool f = False

//and
bool and_ans = t && f==False
printb(and_ans)             //T AND T = T
and_ans = t & f==True
printb(and_ans)             //T AND F = F
and_ans = f && t == False
printb(and_ans)             //F AND F = F


//or
bool or_ans = t || f
print(or_ans)               //T OR T = T
or_ans = t == True || f
print(or_ans)               //T OR F = T
or_ans = f || f
print(or_ans)               //F OR F = F


//not
bool not_ans = !t
print(not_ans)              //NOT TRUE = F
not_ans = !f == True
print(not_ans)              //NOT FALSE = T
