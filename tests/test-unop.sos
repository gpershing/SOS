/*negation of int and float: neg*/
//int
a: int = 5
b: int = -a
print(a) //5
print(b) //-5
print(-1+-2) //-3

//float
c: float = 5.0
d: float = -c
printf(c) //5.0
printf(d) //-5.0
printf(-1.0*-2.0) //2.0


/*negation of bool: not*/
e: bool = True
f: bool = !e
print(e) //True
print(f) //False
print(!!!True) //False
