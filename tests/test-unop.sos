/*negation of int and float: neg*/
//int
int a = 5
int b = -a
print(a) //5
print(b) //-5
print(-1+-2) //-3

//float
float c = 5.0
float d = -c
printf(c) //5.0
printf(d) //-5.0
printf(-1.0*-2.0) //2.0


