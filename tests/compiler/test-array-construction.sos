/*int array*/
//type implicit
array arr = [-2, -1, 0, 1, 2, 3]
print(arr[0]) //-2

//type explicit
array int arrr = [1]
print(arrr[0]) //1


/*float array*/
//type implicit
array arr2 = [5.2]
printf(arr2[0]) //5.2

//type explicit
array float arrr2 = [1.2, 2.4]
printf(arrr2[1]) //2.4


/*bool array*/
//type implicit
array arr3 = [True]
printb(arr3[0]) //True

//type explicit
array bool arrr3 = [False]
printb(arrr3[0]) //False


/*struct array in test-array-of-struct.sos*/


/*nested arrays*/
array array int nested = [[1], [2,3], [4,5,6]]
print(nested[0][0]) //1


/*func arrays*/
int func1() = 5
int func2(int k) = k

array arr4 = [func1(), func2(3)]
print(arr4[0]) //5
